) || 'random'\r\n\r\n    useEffect(() => {\r\n        setLoading(true)            \r\n        getGifs({keyword: keywordToUse})        \r\n          .then(result => {\r\n            setGifs(result)\r\n            setLoading(false)\r\n            // Guardamos la keyword en localStorage\r\n            localStorage.setItem('lastKeyword', keyword)\r\n        })\r\n    }, [keyword, keywordToUse, setGifs])\r\n\r\n    useEffect(() => {\r\n        if (page === INITIAL_PAGE ) return\r\n\r\n        setLoadingNextPage(true)\r\n\r\n        getGifs({keyword: keywordToUse, page })\r\n            .then(nextGifs => {\r\n                setGifs(prevGifs => prevGifs.concat(nextGifs))\r\n                setLoadingNextPage(false)\r\n            })\r\n    }, [keywordToUse, page, setGifs])\r\n\r\n    return { loading, gifs, setPage }\r\n\r\n}\r\n \r\nexport default useGifs;","C:\\Users\\h4rdc\\Documents\\react-giffy\\src\\context\\StaticContext.js",[],"C:\\Users\\h4rdc\\Documents\\react-giffy\\src\\context\\GifsContext.js",[],"C:\\Users\\h4rdc\\Documents\\react-giffy\\src\\components\\Category\\index.js",[],"C:\\Users\\h4rdc\\Documents\\react-giffy\\src\\services\\settings.js",[],"C:\\Users\\h4rdc\\Documents\\react-giffy\\src\\components\\TrendingSearches\\index.js",[],"C:\\Users\\h4rdc\\Documents\\react-giffy\\src\\services\\getTrendingTermsService.js",[],"C:\\Users\\h4rdc\\Documents\\react-giffy\\src\\hooks\\useNearScreen.js",[],"C:\\Users\\h4rdc\\Documents\\react-giffy\\src\\components\\TrendingSearches\\TrendingSearches.js",[],"C:\\Users\\h4rdc\\Documents\\react-giffy\\src\\components\\Spinner\\index.js",[],"C:\\Users\\h4rdc\\Documents\\react-giffy\\src\\components\\SearchForm\\index.js",[],{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","severity":1,"message":"97","line":20,"column":36,"nodeType":"98","endLine":20,"endColumn":47},{"ruleId":"99","severity":1,"message":"100","line":11,"column":13,"nodeType":"98","messageId":"101","endLine":11,"endColu